{
  "name": "b",
  "version": "1.4.3",
  "description": "Backend API",
  "main": "src/index.js",
  "proxy": "http://localhost:4000",
  "scripts": {
    "edit:auth": "code -r src/controllers/authController.js src/routes/authRoutes.js",
    "edit:validate": "code -r src/controllers/authController.js -g validateToken",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,css}\"",
    "check-format": "prettier --check \"**/*.{js,ts,jsx,tsx,json,css}\"",
    "prepare": "exit 0",
    "pretest": "rm -rf logs && mkdir -p logs",
    "test": "jest --forceExit --detectOpenHandles",
    "scan": "node src/utils/dbScan.js",
    "ca": "node src/utils/ca.js",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "version-update": "node scripts/version-control.js",
    "version-major": "node scripts/version-control.js major",
    "version-minor": "node scripts/version-control.js minor",
    "version-patch": "node scripts/version-control.js patch",
    "version:custom": "VERSION_DESC='Custom update' VERSION_CHANGES='Change 1,Change 2' node scripts/version-control.js",
    "version:patch": "node scripts/version-control.js patch",
    "version:minor": "node scripts/version-control.js minor",
    "version:major": "node scripts/version-control.js major",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --runInBand",
    "seed": "node src/utils/dbSeeder.js",
    "check-routes": "tsx src/utils/routeChecker/index.ts"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "archiver": "^5.3.1",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "chartjs-node-canvas": "^4.1.6",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.9",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.9",
    "email-templates": "^11.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.21.2",
    "express-rate-limit": "^6.7.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.2.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.10.0",
    "pdfkit": "^0.13.0",
    "prisma": "^6.3.1",
    "rate-limit-redis": "^3.0.0",
    "sharp": "^0.32.1",
    "stopword": "^2.0.8",
    "uuid": "^11.0.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@prisma/client": "^6.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.2",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,prisma}": [
      "prettier --write"
    ]
  },
  "directories": {
    "test": "tests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
